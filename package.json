{
  "name": "bitminer",
  "version": "0.0.2",
  "description": "가상화폐 채굴, 방치형 키우기 게임입니다.",
  "main": "main.js",
  "devDependencies": {
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "standard": "^11.0.1"
  },
  "scripts": {
    "start": "electron .",
    "standard": "standard",
    "build": "electron-builder",
    "build:dir": "electron-builder --dir"
  },
  "build": {
    "productName": "Bit Miner",
    "appId": "com.electron.bitminer",
    "copyright": "Copyright © 2018 Geunhyeok LEE",
    "directories": {
      "output": "build"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leegeunhyeok/BitMiner.git"
  },
  "author": "Leegeunhyeok",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leegeunhyeok/BitMiner/issues"
  },
  "homepage": "https://github.com/leegeunhyeok/BitMiner#readme"
}
